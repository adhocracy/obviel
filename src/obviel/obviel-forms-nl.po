#: src/obviel/obviel-forms.jslex:135
msgid "1 field did not validate"
msgid_plural "%1 fields did not validate"
msgstr[0] "1 veld kon niet gevalideerd worden"
msgstr[1] "%1 velden konden niet gevalideerd worden"

#: src/obviel/obviel-forms.jslex:1093
msgid "this field is required"
msgstr "Dit veld is verplicht"

#: src/obviel/obviel-forms.jslex:1126
msgid "value too short"
msgstr "Invoer te weinig karakters"

#: src/obviel/obviel-forms.jslex:1129
msgid "value too long"
msgstr "Invoer te veel karakters"

#: src/obviel/obviel-forms.jslex:1193
msgid "not a number"
msgstr "Geen nummer"

#: src/obviel/obviel-forms.jslex:1196
msgid "not an integer number"
msgstr "Geen geheel getal"

#: src/obviel/obviel-forms.jslex:1220 src/obviel/obviel-forms.jslex:1305
#: src/obviel/obviel-forms.jslex:1377
msgid "negative numbers are not allowed"
msgstr "Negatieve getallen zijn niet toegestaan"

#: src/obviel/obviel-forms.jslex:1228
msgid "value must be %1 digits long"
msgstr "De waarde moet %1 cijfers bevatten"

#: src/obviel/obviel-forms.jslex:1266 src/obviel/obviel-forms.jslex:1273
msgid "not a float"
msgstr "Gebruik bij decimalen komma ipv punt"

#: src/obviel/obviel-forms.jslex:1335 src/obviel/obviel-forms.jslex:1345
msgid "not a decimal"
msgstr "Gebruik bij decimalen komma ipv punt"

#: src/obviel/obviel-forms.jslex:1398
msgid "decimal must contain at least %1 digits before the decimal mark"
msgstr "Er zijn tenminste %1 cijfers nodig voor de komma"

#: src/obviel/obviel-forms.jslex:1405
msgid "decimal may not contain more than %1 digits before the decimal mark"
msgstr "Er zijn niet meer dan %1 cijfers toegestaan voor de komma"

#: src/obviel/obviel-forms.jslex:1413
msgid "decimal must contain at least %1 digits after the decimal mark"
msgstr "Er zijn tenminste %1 cijfers nodig na de komma"

#: src/obviel/obviel-forms.jslex:1421
msgid "decimal may not contain more than %1 digits after the decimal mark"
msgstr "Er zijn niet meer dan %1 cijfers toegestaan na de komma"

#: src/obviel/obviel-forms-datepicker.jslex:56
msgid "invalid date"
msgstr "Ongeldige datum"

#: src/obviel/obviel-forms-autocomplete.jslex:152
msgid "unknown value"
msgstr "Ongeldige waarde"
